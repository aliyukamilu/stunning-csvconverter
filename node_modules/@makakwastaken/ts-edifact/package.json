{
  "name": "@makakwastaken/ts-edifact",
  "description": "Edifact parser library",
  "version": "0.1.9",
  "private": false,
  "type": "commonjs",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "edi",
    "edifact",
    "parser"
  ],
  "publisher": "Roman Vottner",
  "readme": "README.md",
  "author": {
    "name": "Roman Vottner"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MakakWasTaken/ts-edifact.git"
  },
  "homepage": "https://github.com/MakakWasTaken/ts-edifact/README.md",
  "bugs": {
    "url": "https://github.com/MakakWasTaken/ts-edifact/-/issues",
    "email": "markus@unknown-studios.com"
  },
  "contributors": [
    {
      "name": "Markus Moltke",
      "email": "markus@unknown-studios.com",
      "url": "https://unknown-studios.com"
    }
  ],
  "scripts": {
    "clean": "rm -rf ./lib ./dist",
    "lint": "eslint --fix . && prettier --write .",
    "build": "tsc -b",
    "check": "npm run check:tsc",
    "check:tsc": "tsc -p . --noEmit",
    "pretest": "npm run lint && npm run build",
    "test": "jasmine-ts --config=spec/support/jasmine.json",
    "coverage": "nyc -r lcov -e .ts -x \"*.spec.ts\" jasmine-ts JASMINE_CONFIG_PATH=spec/support/jasmine.json",
    "prepublishOnly": "yarn build"
  },
  "devDependencies": {
    "@initics/tsm": "^1.0.2",
    "@types/events": "3.0.0",
    "@types/htmlparser2": "^3.10.1",
    "@types/jasmine": "^4.3.0",
    "@types/node": "^18.11.0",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@typescript-eslint/typescript-estree": "^5.40.0",
    "domhandler": "^5.0.3",
    "eslint": "^8.25.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "events": "^3.1.0",
    "glob-parent": "^6.0.1",
    "jasmine": "^3.9.0",
    "jasmine-core": "^3.9.0",
    "jasmine-node": "https://github.com/MakakWasTaken/jasmine-node",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "jasmine-ts-console-reporter": "^3.1.1",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.1.1",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.4.2",
    "webpack": "^5.74.0"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "htmlparser2": "^7.2.0"
  },
  "files": [
    "lib/**/*"
  ]
}
